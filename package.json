{
  "name": "@ekinolik/jax-react-client",
  "version": "1.0.0",
  "description": "React client for JAX API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "github:ekinolik/jax-react-client"
  },
  "files": [
    "dist",
    "src/generated",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "generate": "protoc -I=./proto ./proto/dex/v1/dex.proto --js_out=import_style=commonjs:src/generated --grpc_out=grpc_js:src/generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin`",
    "prepare": "npm run build",
    "prepublishOnly": "npm run generate && npm run build",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "pretest:integration": "npm run generate && npm run build"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "google-protobuf": "^3.21.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "grpc-tools": "^1.12.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "react": "^18.2.0"
  }
}
